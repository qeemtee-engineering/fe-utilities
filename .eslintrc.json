{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": "off",
    "class-methods-use-this": "off",
    "no-console": "off",
    "no-param-reassign": "off",
    "import/prefer-default-export": "off",
    "arrow-parens": "off"
  }
}
